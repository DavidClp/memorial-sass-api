{
  "name": "memorial-sass-api",
  "version": "1.0.0",
  "description": "API do EternoMemorial",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/shared/infra/http/express/server.ts",
    "dev:watch": "nodemon --exec ts-node src/index.ts",
    "db:push": "prisma db push",
    "db:pull": "prisma db pull && prisma generate",
    "db:generate": "prisma generate",
    "build": "tsc && tsc-alias",
    "start": "node build/index.js",
    "build-windows": "npm remove sharp && npm add sharp@0.33.3 --ignore-engines && rimraf build && npm db:generate && tsc"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.676.0",
    "@prisma/client": "^6.19.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "sharp": "^0.34.5",
    "uuid": "^11.0.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "prisma": "^6.19.0",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  }
}
